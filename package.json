{
  "name": "puttyimages-web",
  "version": "1.0.0",
  "description": "An open stock image DApp",
  "author": "Rick Mak <rick.mak@gmail.com>",
  "private": true,
  "scripts": {
    "build": "nuxt build && backpack build",
    "eslint": "eslint --ignore-path .gitignore '{**/*,*}.{js,vue}'",
    "dev": "backpack dev",
    "lint": "npm run stylelint && npm run eslint",
    "lint:fix": "npm run stylelint -- --fix && npm run eslint -- --fix",
    "precommit": "npm run lint",
    "sequelize": "sequelize",
    "sequelize:reset": "npm run sequelize db:migrate:undo:all && npm run sequelize db:migrate",
    "sequelize:reseed": "npm run sequelize db:seed:undo:all && npm run sequelize db:seed:all",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "stylelint": "stylelint -i .gitignore '**/*.{scss,vue}'",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@likecoin/ethereum-blockies": "^0.1.2",
    "@likecoin/likecoin-eth-util": "^1.0.1",
    "@nuxtjs/sentry": "^1.1.0",
    "axios": "^0.18.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "bs58": "^4.0.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.6",
    "date-fns": "^1.29.0",
    "eth-sig-util": "^2.0.2",
    "ethereumjs-abi": "^0.6.5",
    "exif-parser": "^0.1.12",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "file-saver": "^1.3.8",
    "image-size": "^0.6.3",
    "image-type": "^3.0.0",
    "ipfs-api": "^24.0.1",
    "jest": "^23.1.0",
    "js-base64": "^2.4.5",
    "js-sha256": "^0.9.0",
    "json-stable-stringify": "^1.0.1",
    "jsonwebtoken": "^8.3.0",
    "lodash.throttle": "^4.1.1",
    "multer": "^1.3.0",
    "node-sass": "^4.9.0",
    "nuxt": "^1.4.1",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sass-loader": "^7.0.2",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0",
    "source-map-support": "^0.5.6",
    "umzug": "^2.1.0",
    "vue": "^2.5.17",
    "vue-awesome-swiper": "^3.1.3",
    "vue-clipboard2": "^0.2.0",
    "vue-cookie": "^1.1.4",
    "vue-i18n": "^8.0.0",
    "vue-infinite-loading": "^2.3.1",
    "vue-server-renderer": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "vuetify": "^1.0.18",
    "vuex": "^3.0.1",
    "web3-utils": "^1.0.0-beta.34",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "axiosist": "^0.1.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.0.1",
    "backpack-core": "^0.7.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.1.0",
    "vue-svg-loader": "^0.8.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "server/**/*.js",
      "!server/seeders/*.js"
    ],
    "coverageDirectory": "./coverage/",
    "globalSetup": "<rootDir>/__tests__/setup.js",
    "testMatch": [
      "<rootDir>/__tests__/**/*.test.js"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
